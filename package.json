    "test:e2e-debug": "playwright test --debug",
    "test:e2e-report": "playwright show-report",
    "test:e2e-auth": "playwright test tests/e2e/auth.spec.ts",
    "test:e2e-conversation": "playwright test tests/e2e/conversation.spec.ts",
    "test:e2e-governance": "playwright test tests/e2e/governance.spec.ts",
    "test:e2e-performance": "playwright test tests/e2e/performance.spec.ts",
    "deploy:staging": "node scripts/deploy-netlify.js",
    "deploy:production": "node scripts/deploy-elest.js",
    "deploy:check": "npm run lint && npm run build && npm run test:supabase-integration",
    "deploy:prepare": "npm run deploy:check && npm run test:e2e-auth",
    "docker:dev": "node scripts/docker-manager.js start dev",
    "docker:local": "node scripts/docker-manager.js start local",
    "docker:stop": "node scripts/docker-manager.js stop",
    "docker:clean": "node scripts/docker-manager.js clean",
    "docker:status": "node scripts/docker-manager.js status",
    "docker:logs": "node scripts/docker-manager.js logs",
    "security:audit": "node scripts/security-audit.js",
    "test:user-journey": "node scripts/user-journey-test.js",
    "test:security": "npm run security:audit && npm run test:user-journey",
    "pre-deploy": "npm run security:audit && npm run test:user-journey && npm run build",
    "setup:supabase": "node scripts/setup-supabase-production.js",
    "deploy:production": "npm run setup:supabase && npm run pre-deploy"
